<Window x:Class="Automotora.ListadoAutomoviles"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Automotora"
        mc:Ignorable="d"
        Title="Listado automoviles" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgAutomoviles" HorizontalAlignment="Left" Height="242" Margin="78,108,0,0" VerticalAlignment="Top" Width="625" IsReadOnly="True"/>
        <Label x:Name="lblPatente" Content="Patente" HorizontalAlignment="Left" Margin="78,42,0,0" VerticalAlignment="Top" Height="31" Width="57" FontWeight="Bold"/>
        <TextBox x:Name="txtPatente" HorizontalAlignment="Left" Height="23" Margin="151,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyUp="txtPatente_KeyUp"/>

    </Grid>
</Window>
